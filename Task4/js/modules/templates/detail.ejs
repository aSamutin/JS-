<div class="detail-ticket">  
  <input type="submit" value="Назначить исполнителя" id="openSelectExecutor">
  <input type="submit" value="Изменить заявку" id="openEditTicket">
  <table>
    <caption><h2>Заявка № <span><%=ticket.id%></span></h2><caption>
    <thead>
    <tr>
        <th>Клиент</th>
        <th>Исполнитель</th>
        <th>Описание</th>
        <th>Приоритет</th>
        <th>Estimated</th>
        <th>Deadline</th>
        <th>Готовность (%)</th>
        <th>Статус</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <%=ticket.clientId%>
        </td>
        <td>
          <%=ticket.executorId%>
        </td>
        <td>
          <%=ticket.description%>
        </td>
        <td>
          <%=ticket.priority%>
        </td>
        <td>
          <%=ticket.estimated%>
        </td>
        <td>
          <%=ticket.deadline%>
        </td>
        <td>
          <%=ticket.percentReady%>
        </td>
        <td>
          <%=ticket.status%>
        </td>
      </tr> 
    </tbody>
    </table> 
    <div class="comments">
      <h2>Комментарии</h2>
      <% for( var i = 0; i < comment.length; i++) {%>
        <div class="comment">
            <h3><%=comment[i].userId%></h3>
            <div><%=comment[i].text%></div>
        </div>
      <% }; %>
    </div>
    <input type="submit" value="Добавить комментарий" id="openCreateComment">
</div>